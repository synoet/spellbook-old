generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Command {
  id          String @id @unique 
  content     String
  description String
  labels      String[]
  createdAt   DateTime @default(now()) 
  updatedAt   DateTime @updatedAt
}

model Book {
  id          String @id @unique
  title       String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now())
  labels      String[]
}
